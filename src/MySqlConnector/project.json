{
	"version": "0.9.2",
	"title": "Async MySQL Connector",
	"description": "A fully async MySQL ADO.NET provider for .NET and .NET Core",
	"copyright": "Copyright 2016 Bradley Grainger",
	"buildOptions": {
		"warningsAsErrors": true
	},
	"packOptions": {
		"owners": [ "bgrainger" ],
		"tags": [ "mysql", "async", "ado.net", "database", "netcore"  ],
		"releaseNotes": "Fix MySqlConnection.DataSource with Unix Domain Socket. (GitHub issue #152)",
		"licenseUrl": "https://opensource.org/licenses/MIT",
		"projectUrl": "https://github.com/mysql-net/MySqlConnector",
		"repository": {
			"type": "git",
			"url": "https://github.com/mysql-net/MySqlConnector.git"
		}
	},
	"authors": [ "Bradley Grainger", "Caleb Lloyd" ],
	"frameworks": {
		"net451": {
			"frameworkAssemblies": {
				"System.Data": {
					"version": "4.0.0.0",
					"type": "build"
				},
				"System.Transactions": {
					"version": "4.0.0.0",
					"type": "build"
				}
			},
			"dependencies": {
				"System.Buffers": "4.0.0",
				"System.Runtime.InteropServices.RuntimeInformation": "4.0.0",
				"System.Threading.Tasks.Extensions": "4.0.0"
			}
		},
		"netstandard1.3": {
			"dependencies": {
				"System.Buffers": "4.0.0",
				"System.Collections.Concurrent": "4.0.12",
				"System.Data.Common": "4.1.0",
				"System.IO": "4.1.0",
				"System.IO.Compression": "4.1.0",
				"System.IO.FileSystem": "4.0.1",
				"System.Linq": "4.1.0",
				"System.Net.NameResolution": "4.0.0",
				"System.Net.Security": "4.0.0",
				"System.Net.Sockets": "4.1.0",
				"System.Runtime.Extensions": "4.1.0",
				"System.Runtime.InteropServices.RuntimeInformation": "4.0.0",
				"System.Text.RegularExpressions": "4.1.0",
				"System.Threading.Tasks.Extensions": "4.0.0"
			}
		}
	}
}
