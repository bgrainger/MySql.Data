{
	"version": "0.2.1",
	"title": "Async MySQL Connector",
	"description": "A fully async MySQL ADO.NET provider for .NET and .NET Core",
	"copyright": "Copyright 2016 Bradley Grainger",
	"buildOptions": {
		"warningsAsErrors": true,
		"additionalArguments": [ "/ruleset:stylecop.ruleset" ]
	},
	"packOptions": {
		"owners": [ "bgrainger" ],
		"tags": [ "mysql", "async", "ado.net", "database", "netcore" ],
		"releaseNotes": "Provide more information for unsupported auth plugins.",
		"licenseUrl": "https://opensource.org/licenses/MIT",
		"projectUrl": "https://github.com/bgrainger/MySqlConnector",
		"repository": {
			"type": "git",
			"url": "https://github.com/bgrainger/MySqlConnector.git"
		}
	},
	"authors": [ "Bradley Grainger", "Caleb Lloyd" ],
	"frameworks": {
		"net45": {
			"frameworkAssemblies": {
				"System.Data": "4.0.0.0",
				"System.Transactions": "4.0.0.0"
			},
			"dependencies": {
				"System.Threading.Tasks.Extensions": "4.0.0",
				"StyleCop.Analyzers": {
					"version": "1.0.0",
					"type": "build"
				}
			}
		},
		"netstandard1.3": {
			"frameworkAssemblies": {
			},
			"dependencies": {
				"System.Collections.Concurrent": "4.0.12",
				"System.Data.Common": "4.1.0",
				"System.Globalization": "4.0.11",
				"System.IO": "4.1.0",
				"System.Linq": "4.1.0",
				"System.Net.NameResolution": "4.0.0",
				"System.Net.Sockets": "4.1.0",
				"System.Runtime": "4.1.0",
				"System.Runtime.Extensions": "4.1.0",
				"System.Security.Cryptography.Algorithms": "4.2.0",
				"System.Threading": "4.0.11",
				"System.Threading.Tasks.Extensions": "4.0.0",
				"StyleCop.Analyzers": {
					"version": "1.0.0",
					"type": "build"
				}
			}
		}
	}
}
